
#include <stdio.h>

int main()
{
    int a[100];
    int n;
    int v;
    int count = 0;

    scanf_s("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf_s("%d", &a[i]);
    }

    scanf_s("%d", &v);

    for (int i = 0; i < n; i++) {
        if (a[i] == v)
            count++;
            

    }
    printf("%d", count);
    return 0;
}
